<!DOCTYPE html>
<html>
<head>
  <title>Mylife</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<header>
 <% flash.each do |key, message| %>
   <div class="alert alert-<%= key %>"><%= message %> </div>
 <% end %>

</header>
<div class="container-fluid">
<%= yield %>
</div>
</body>
</html>

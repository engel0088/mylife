<!DOCTYPE html>
<html>
<head>
  <title>Mylife</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container-fluid">
  <% flash.each do |key, value| %>
     <div class="alert alert-info">
       <%= value %>
     </div>
  <% end %>
  <%= yield %>
</div>

</body>
</html>
